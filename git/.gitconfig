[user]
	name = Dante Briones
[include]
	path = ~/.gitconfig.local
[core]
	editor = "/usr/local/bin/atom --wait"
	autocrlf = input
	excludesfile = ~/.gitignore_global
[alias]
	ci = commit
	st = status
	co = checkout
[credential]
	helper = osxkeychain
[push]
	default = tracking
[branch]
	autosetuprebase = always
